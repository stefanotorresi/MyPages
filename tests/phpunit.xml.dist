<?xml version="1.0" encoding="UTF-8"?>
<phpunit bootstrap="Bootstrap.php" colors="true">
    <testsuite name="MyPages Test Suite">
        <directory>./MyPagesTest</directory>
    </testsuite>

    <filter>
        <whitelist>
            <directory>../src/</directory>
        </whitelist>
    </filter>

    <logging>
        <log
            type="coverage-html"
            target="./report"
            charset="UTF-8"
            yui="true"
            hightlight="true"
            lowUpperBound="50"
            highLowerBound="80"
            />
    </logging>
</phpunit>
